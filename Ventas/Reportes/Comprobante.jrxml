	<queryString>
		<![CDATA[select *,ce.email as EmailCorporativo,nombreUbigeo(e.ubigeo)as Localizacion, precioUnit*cantidad as TotalProd from comprobantePago cp inner join 	Producto_has_comprobantepago pc 
									on 	Comprobantepago_serieComprobante = serieComprobante and
								   		Comprobantepago_Empresa_idEmpresa= Empresa_idEmpresa and
								   		Comprobantepago_TipoComprobante_idTipoComprobante = TipoComprobante_idTipocomprobante
								 inner join Producto
								 	on Producto_idProducto=idProducto
								 inner join Cliente
								 	on Cliente_numero=numero
								 inner join Empresa e
								 	on cp.Empresa_idEmpresa=idEmpresa
inner join TipoComprobante tc
	on cp.TipoComprobante_idTipoComprobante=tc.idTipoComprobante
inner join CorreoEmpresa ce
	on e.idEmpresa=ce.Empresa_idEmpresa
where ComprobantePago_serieComprobante=$P{ComprobantePago_serieComprobante}
and cp.Empresa_idEmpresa=$P{Empresa_idEmpresa}
and TipoComprobante_idTipoComprobante=$P{TipoComprobante_idTipoComprobante}]]>
	</queryString>
